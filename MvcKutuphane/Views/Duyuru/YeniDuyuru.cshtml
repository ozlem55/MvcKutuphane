@model MvcKutuphane.Models.Entity.TBLDUYURU
@{
    ViewBag.Title = "YeniDuyuru";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>YeniDuyuru</h2>


<form class="form-group" method="post">
    <div style="margin-top:15px">
        <label>Duyuru Başlığı </label>
        @Html.TextBoxFor(x => x.KATEGORI, new { @class = "form-control" })

    </div>
    <div style="margin-top:15px">
        <label>Duyuru İçeriği</label>
        @Html.TextBoxFor(x => x.ICERIK, new { @class = "form-control" })

    </div>
    <div style="margin-top:15px">
        <label>Duyuru Traihi </label>
        @Html.TextBoxFor(x => x.TARIH, new { @class = "form-control" })

    </div>
    <div style="margin-top:15px">
        <button class=" btn btn-info" id="Btn1">Duyuru Ekle</button>
    </div>
</form>
